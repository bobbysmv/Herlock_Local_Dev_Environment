extends layout
block content
    h1= title
    table
        each val, key in list
            tr
                td
                    b
                        a(href=val.urlscheme)=key
                td
                    a(href=val.preview target='_blank') webpreview
                td
                    a(href=val.doc target='_blank') doc
                td
                    a(href=val.force target='_blank') doc更新
            tr
                td(colspan="4" style="text-indent:1em")=val.url

    div(id="qr")
        !{qr}

